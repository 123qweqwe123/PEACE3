  <select id="getUqsCompleteTab" parameterType="java.util.Map" resultType="java.util.Map">
	select 
		lcc.lcc_code as lccCode, 
		lcc.lcc_name as lccName,
		p.patient_id as patientId,
		p.patient_name as patientName,
		trunc(sysdate)-trunc(t.end_time) as days,
		decode(l.state, 1, '完成', '未完成') as state
	from 
    	pip_comm_patient p 
	left join 
		pip_comm_lcc lcc
	on lcc.lcc_code = p.lcc_code
  	left join 
    	(
	    	select patient_id, state
	    	from pip_uqs_answerqn_log 
	    	where uqs_version = '004.012.001'
	    	group by patient_id, state
    	) l
    on l.patient_id = p.patient_id
    left join 
    	(
    		select patient_id, max(end_time) as end_time
	    	from pip_uqs_answerqn_log 
	    	where uqs_version = '004.011.001' and state = 1
	    	group by patient_id
    	) t
  	on t.patient_id = p.patient_id
	where 
		( p.is_diabetes != 1 or p.is_diabetes is null  )
    	and p.is_join_msg = '1'
    	
		<if test="lccCode != null and lccCode != ''">
	        and p.lcc_code = #{lccCode, jdbcType=VARCHAR}
	    </if>
	    
		and exists(
			select 1 from pip_uqs_answerqn_log ql where ql.uqs_version = '004.011.001' 
			and ql.state = 1 and p.patient_id = ql.patient_id and sysdate - end_time > 2
		)
		
		and not exists (
			select 1 from pip_uqs_answerqn_log ql where ql.uqs_version = '004.012.001' 
			and p.patient_id = ql.patient_id
	        and (ql.state = 1 or p.is_dead = 1)
	    )
	    
		<if test="sidx != null and sidx != ''">
            order by ${sidx}
            <if test="sord!=null and sord!= ''">
                ${sord}
            </if>
        </if>
  </select>
