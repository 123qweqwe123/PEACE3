package com.bdcor.pip.web.pro.promgt.domain;

import javax.persistence.Transient;

public class LccUser {
	private String userCode;
	private String name;
	private String phone;
	private String email;
	private String lccCode;
	private String lccRole;
	private String isSignSigna;
	private String isGetResume;
	private String isJoinTraining;
	private String status;
	private String projectId;
	private String mobile;

	private String addToLinkMan;
	private String linkManCode;
	private String researchStatus; // 参严人员1 离严人员0
	private String helpCode;

	private String lccRoleType; // --单位人员类型
	@Transient
	private String projectName;
	@Transient
	private String lccName;

	public LccUser() {
		super();
	}

	public void setLccRoleType(String lccRoleType) {
		this.lccRoleType = lccRoleType;
	}

	public String getLccRoleType() {
		return lccRoleType;
	}

	public LccUser(String userCode, String name, String phone, String email,
				   String lccCode, String isSignSigna, String isGetResume,
				   String isJoinTraining, String status, String projectId,
				   String mobile, String addToLinkMan, String linkManCode,
				   String helpCode, String lccRole, String researchStatus) {
		super();
		this.userCode = userCode;
		this.name = name;
		this.phone = phone;
		this.email = email;
		this.lccCode = lccCode;
		this.isSignSigna = isSignSigna;
		this.isGetResume = isGetResume;
		this.isJoinTraining = isJoinTraining;
		this.status = status;
		this.projectId = projectId;
		this.mobile = mobile;
		this.addToLinkMan = addToLinkMan;
		this.linkManCode = linkManCode;
		this.helpCode = helpCode;
		this.lccRole = lccRole;
		this.researchStatus = researchStatus;
	}

	public String getUserCode() {
		return userCode;
	}

	public void setUserCode(String userCode) {
		this.userCode = userCode;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getPhone() {
		return phone;
	}

	public void setPhone(String phone) {
		this.phone = phone;
	}

	public String getResearchStatus() {
		return researchStatus;
	}

	public void setResearchStatus(String researchStatus) {
		this.researchStatus = researchStatus;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getLccCode() {
		return lccCode;
	}

	public void setLccCode(String lccCode) {
		this.lccCode = lccCode;
	}

	public String getLccRole() {
		return lccRole;
	}

	public void setLccRole(String lccRole) {
		this.lccRole = lccRole;
	}

	public String getIsSignSigna() {
		return isSignSigna;
	}

	public void setIsSignSigna(String isSignSigna) {
		this.isSignSigna = isSignSigna;
	}

	public String getIsGetResume() {
		return isGetResume;
	}

	public void setIsGetResume(String isGetResume) {
		this.isGetResume = isGetResume;
	}

	public String getIsJoinTraining() {
		return isJoinTraining;
	}

	public void setIsJoinTraining(String isJoinTraining) {
		this.isJoinTraining = isJoinTraining;
	}

	public String getStatus() {
		return status;
	}

	public void setStatus(String status) {
		this.status = status;
	}

	public String getProjectId() {
		return projectId;
	}

	public void setProjectId(String projectId) {
		this.projectId = projectId;
	}

	public String getMobile() {
		return mobile;
	}

	public void setMobile(String mobile) {
		this.mobile = mobile;
	}

	public String getProjectName() {
		return projectName;
	}

	public void setProjectName(String projectName) {
		this.projectName = projectName;
	}

	public String getAddToLinkMan() {
		return addToLinkMan;
	}

	public void setAddToLinkMan(String addToLinkMan) {
		this.addToLinkMan = addToLinkMan;
	}

	public String getLinkManCode() {
		return linkManCode;
	}

	public void setLinkManCode(String linkManCode) {
		this.linkManCode = linkManCode;
	}

	public String getLccName() {
		return lccName;
	}

	public void setLccName(String lccName) {
		this.lccName = lccName;
	}

	public String getHelpCode() {
		return helpCode;
	}

	public void setHelpCode(String helpCode) {
		this.helpCode = helpCode;
	}

}
