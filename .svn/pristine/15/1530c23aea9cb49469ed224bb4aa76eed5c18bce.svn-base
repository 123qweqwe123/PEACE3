package com.bdcor.pip.client.tools;



import java.sql.Connection;

public interface IConnect {
	
	/**
	 * 初始化配置信息
	 */
	public void initParam();
	
	/**
	 * 
	 * 初始化数据库连接
	 */
	public void initConnect();
	
	/**
	 * 关闭数据库连接，并关闭数据库
	 */
	public void closeCon();
	
	/**
	 * 获得数据库连接
	 * @return
	 */
	public Connection getCon();

	
}
